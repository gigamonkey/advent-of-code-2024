{
  "trailingComma": "all",
  "tabWidth": 2,
  "bracketSpacing": true,
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "plugins": ["prettier-plugin-java"],
  "arrowParens": "always",
  "overrides": [
    {
      "files": ["*.java"],
      "options": {
        "parser": "java"
      }
    }

  ]

}
